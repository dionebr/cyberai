{
	"name": "cyberai-offensive-assistant",
	"version": "1.0.0",
	"description": "CyberAI Offensive Assistant Web",
	"main": "src/web/static/js/main.js",
	"scripts": {
		"start": "python3 -m http.server 8080 --directory src/web",
